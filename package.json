{
  "name": "nkdayscraper",
  "version": "0.6.1",
  "private": true,
  "scripts": {
    "serve-prod": "vue-cli-service serve",
    "build-prod": "vue-cli-service build",
    "lint-prod": "vue-cli-service lint",
    "serve": "vue-cli-service serve --mode development",
    "build": "vue-cli-service build --mode development",
    "lint": "vue-cli-service lint --mode development",
    "jrr": "scrapy crawl jrarecords -o data/json/jrarecords.json",
    "nkd": "scrapy crawl nkday -o data/json/nkday.json",
    "flask": "python3 app.py",
    "flask-dev": "SQLALCHEMY_WARN_20=1 python3 -W always::DeprecationWarning app.py",
    "scrape": "npm run jrr && npm run nkd",
    "app": "npm run build && npm run flask",
    "deploy": "npm run scrape && npm run app",
    "zip": "python3 nkdayscraper/utils/ziptools.py data/json",
    "all": "npm run scrape && npm run zip && npm run app"
  },
  "dependencies": {
    "@popperjs/core": "^2.6.0",
    "axios": "^0.21.1",
    "bootstrap": "^5.0.0-beta1",
    "core-js": "^3.8.1",
    "jquery": "^3.5.1",
    "typescript": "^3.9.7",
    "vue": "^3.0.4"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.9",
    "@vue/cli-plugin-eslint": "^4.5.9",
    "@vue/cli-service": "^4.5.12",
    "@vue/compiler-sfc": "^3.0.4",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.3.0",
    "node-sass": "^5.0.0",
    "sass-loader": "^10.1.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
